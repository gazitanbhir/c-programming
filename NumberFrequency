#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,j,n,f=1;
    printf("No. of elements : ");
    scanf("%d",&n);
    int num[n],fr[50];
    for(i=0; i<n; i++)
    {
        scanf("%d",&num[i]);
    }

    for(i=0; i<n; i++)
    {
        for(j=i+1; j<n; j++)
        {
            if(num[i]==num[j])
            {
                f++;
                num[j]=0;
            }
        }
        fr[i]=f;
        f=1;

        if(num[i]) printf("%d : %d \n",num[i],fr[i]);
    }

    return 0;
}



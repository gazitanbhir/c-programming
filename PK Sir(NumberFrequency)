#include <stdio.h>
#include <stdlib.h>
b[1000]= {0};
int main()
{
    int n,i,a[100];
    scanf("%d",&n);
    for(i=1; i<=n; i++)
    {
        scanf("%d",&a[i]);
        b[a[i]]++;
    }
    for(a[i]=0; a[i]<=100; a[i]++)
    {
        if(b[a[i]]) printf("%d = %d \n",a[i],b[a[i]]);
    }
    return 0;
}

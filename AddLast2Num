#include <stdio.h>
#include <stdlib.h>
#include<string.h>
int main()
{
    int i,i2,j,j2,x,y,n1,n2;
    char num1[1000],num2[1000];
    gets(num1);
    gets(num2);
    x=strlen(num1);
    y=strlen(num2);
    i=num1[x-1]-48;
    i2=num1[x-2]-48;

    j=num2[y-1]-48;
    j2=num2[y-2]-48;

    n1=i2*10+i;
    n2=j2*10+j;

    printf("%d",n1+n2);

    return 0;
}
